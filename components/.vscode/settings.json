// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "**/*.map": {
            "when": "$(basename)"
        },
        "**/*.css": {
            "when": "$(basename).scss"
        },
        "**/node_modules": true,
        "**/dist": true,
        "src/**/*.d.ts": true
    }
}