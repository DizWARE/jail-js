    "vue-property-decorator": "^7.0.0",
    "vuetify": "^1.3.0"
  },
  "devDependencies": {
    "@vue/cli": "^3.0.5",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-typescript": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "typescript": "^3.0.0",
    "vue-cli-plugin-vuetify": "^0.4.2",
    "vue-template-compiler": "^2.5.17",
    "vuetify-loader": "^1.0.5"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
