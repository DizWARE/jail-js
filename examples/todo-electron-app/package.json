{
  "name": "electron-typescript-boilerplate",
  "version": "0.0.1",
  "description": "A stripped down boilerplate for a typescript electron application.",
  "main": "app/main.js",
  "dependencies": {
    "electron-is": "2.4.x",
    "jail-js": "../.."
  },
  "devDependencies": {
    "@types/node": "7.0.0",
    "electron": "2.0.x",
    "gulp": "3.9.x",
    "gulp-sass": "3.1.x",
    "gulp-sourcemaps": "2.4.x",
    "gulp-typescript": "3.1.x",
    "node-sass": "4.9.x",
    "typescript": "2.1.x"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:ts": "tsc -p .",
    "build:gulpfile": "tsc gulpfile",
    "build:rebuild": "npm install && tsc -p .",
    "build:gulp": "gulp build",
    "build:gulp:rebuild": "npm uninstall jail-js && npm install && gulp build",
    "start": "electron ."
  },
  "author": "DizWARE (Tyler Robinson)",
  "license": "MIT"
}
