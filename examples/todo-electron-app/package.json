{
  "name": "electron-typescript-boilerplate",
  "version": "0.0.1",
  "description": "A stripped down boilerplate for a typescript electron application.",
  "main": "app/main.js",
  "dependencies": {
    "electron-is": "^2.4.0",
    "jail-js": "git+https://dizware.visualstudio.com/_git/jail-js"
  },
  "devDependencies": {
    "@types/electron": "1.4.31",
    "@types/node": "7.0.0",
    "electron": "^1.5.0",
    "gulp": "^3.9.1",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.4.0",
    "gulp-typescript": "^3.1.4",
    "node-sass": "^4.5.0",
    "typescript": "^2.1.5"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:ts": "tsc -p .",
    "build:gulpfile": "tsc gulpfile",
